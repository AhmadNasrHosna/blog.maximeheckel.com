{
  "name": "blog.maximeheckel.com",
  "private": true,
  "description": "blog.maximeheckel.com - Blog",
  "version": "0.4.0",
  "author": "Maxime Heckel <hello@maximeheckel.com>",
  "dependencies": {
    "@emotion/core": "^10.0.6",
    "@emotion/styled": "^10.0.6",
    "@mdx-js/mdx": "^1.1.6",
    "@mdx-js/react": "^1.1.6",
    "@sindresorhus/slugify": "^0.9.1",
    "emotion-theming": "^10.0.6",
    "focus-trap-react": "^6.0.0",
    "gatsby": "2.17.7",
    "gatsby-image": "2.2.19",
    "gatsby-plugin-google-analytics": "^2.0.9",
    "gatsby-plugin-lunr": "^1.5.2",
    "gatsby-plugin-manifest": "2.2.8",
    "gatsby-plugin-offline": "^2.2.9",
    "gatsby-plugin-printer": "1.0.8",
    "gatsby-plugin-react-helmet": "^3.1.3",
    "gatsby-plugin-sitemap": "2.2.6",
    "gatsby-plugin-typescript": "^2.0.3",
    "gatsby-source-filesystem": "^2.1.9",
    "gatsby-theme-maximeheckel": "0.1.47",
    "mousetrap": "^1.6.3",
    "prop-types": "^15.7.2",
    "react": "16.8.0",
    "react-dom": "16.8.0",
    "react-typist": "^2.0.5",
    "react-typography": "^0.16.18",
    "typography": "^0.16.18"
  },
  "devDependencies": {
    "@testing-library/react": "^8.0.7",
    "@types/jest": "^23.0.0",
    "@types/mousetrap": "^1.6.3",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "@types/react-helmet": "^5.0.8",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "babel-jest": "^23.1.0",
    "babel-preset-gatsby": "^0.1.7",
    "cypress": "4.0.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "identity-obj-proxy": "^3.0.0",
    "is-ci-cli": "^1.1.1",
    "jest": "^23.0.1",
    "jest-dom": "^3.0.2",
    "jest-emotion": "^10.0.7",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.8.1",
    "start-server-and-test": "^1.7.11",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.4"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "rm -rf .cache && gatsby build",
    "develop": "rm -rf .cache && gatsby develop",
    "format": "prettier --write \"src/**/*.{js|ts|tsx}\"",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "cy:open": "cypress open",
    "cy:run": "cypress run --browser chrome",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:e2e": "is-ci \"test:e2e:ci\" \"test:e2e:dev\"",
    "test:e2e:dev": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cy:run",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ext=jsx,ts,tsx src",
    "validate": "npm run test:coverage && npm run test:e2e:ci"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "jest": {
    "snapshotSerializers": [
      "jest-emotion"
    ],
    "setupTestFrameworkScriptFile": "./setup-jest.js",
    "globals": {
      "__PATH_PREFIX__": "",
      "ts-jest": {
        "diagnostics": false,
        "tsConfig": "tsconfig.test.json"
      }
    },
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(tsx?|jsx?)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js|jsx)$",
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "typeface-*": "identity-obj-proxy",
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file.js"
    },
    "collectCoverage": false,
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ]
  }
}
